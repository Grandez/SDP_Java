/**********************************************************************/
/**********************************************************************/
/**********************************************************************/
/*                        INICIO PARSER                               */
/***********************S***********************************************/
/**********************************************************************/
/**********************************************************************/

cics ::= cics_statement:s  {: code.processCICSStatement(stmt);
                              RESULT = symbolFactory.newSymbol(0, 0, 0, stmt);  
                           :}   
     |                     {: RESULT = symbolFactory.newSymbol(0, 0, 0, null); :}        
     ;
              
cics_statement ::= cics_stmt:s cics_options:o {: RESULT = s; :}  
               ;

cics_stmt ::= cics_area_pgm:s      {: stmt.setGroup(CDG.STMT_CICS_PGM);   RESULT = s; :}
          |   cics_area_program:s  {: stmt.setGroup(CDG.STMT_CICS_CTL);   RESULT = s; :}
          |   cics_area_web:s      {: stmt.setGroup(CDG.STMT_CICS_WEB);   RESULT = s; :}
          |   cics_area_sys:s      {: stmt.setGroup(CDG.STMT_CICS_SYS);   RESULT = s; :}         
          |   cics_stmt_common:s   {: stmt.setGroup(CDG.STMT_CICS_PGM);   RESULT = s; :}  
          ;
                        
/**********************************************************************/
/*                        AREAS                                       */
/**********************************************************************/

cics_area_pgm ::= cics_group_abend:s               {: RESULT = s; :}
              |   cics_group_auth               :s {: RESULT = s; :}
              |   cics_group_bms                :s {: RESULT = s; :}
              |   cics_group_bts                :s {: RESULT = s; :}
              |   cics_group_diag               :s {: RESULT = s; :}
              |   cics_group_excep              :s {: RESULT = s; :}
              |   cics_group_file               :s {: RESULT = s; :}
              |   cics_group_interval           :s {: RESULT = s; :}
              |   cics_group_journaling         :s {: RESULT = s; :}
              |   cics_group_schedul            :s {: RESULT = s; :}
              |   cics_group_spool              :s {: RESULT = s; :}
              |   cics_group_storage            :s {: RESULT = s; :}
              |   cics_group_task               :s {: RESULT = s; :}
              |   cics_group_ts                 :s {: RESULT = s; :}
              |   cics_group_terminal           :s {: RESULT = s; :}
              ;

cics_area_program ::=  LINK:w      {: stmt = new StmtCICS(w); :}
                   |   LOAD:w      {: stmt = new StmtCICS(w); :}
                   |   RELEASE:w   {: stmt = new StmtCICS(w); :}
                   |   RETURN:w    {: stmt = new StmtCICS(w); :}
                   |   XCTL:w      {: stmt = new StmtCICS(w); :}
                   ;

cics_area_web ::= CONVERTTIME:w                    {: stmt = new StmtCICS(w);         :}
               |   WEB:w cics_web:w2               {: stmt = new StmtCICS(w.add(w2)); :}    
               |   INVOKE:w cics_invoke:w2         {: stmt = new StmtCICS(w.add(w2)); :}    
               |   SOAPFAULT:w cics_soap:w2        {: stmt = new StmtCICS(w.add(w2)); :}    
               |   TRANSFORM:w cics_transform:w2   {: stmt = new StmtCICS(w.add(w2)); :}    
               |   WSACONTEXT:w cics_wsacontext:w2 {: stmt = new StmtCICS(w.add(w2)); :}    
               |   WSAEPR:w CREATE:w2              {: stmt = new StmtCICS(w.add(w2)); :}    
               ;

cics_area_sys ::= cics_stmt_acquire:s    {: RESULT = s; :} 
              |   cics_stmt_collect:s    {: RESULT = s; :} 
              |   cics_stmt_create:s     {: RESULT = s; :}  
              |   cics_stmt_disable:s    {: RESULT = s; :}
              |   cics_stmt_discard:s    {: RESULT = s; :} 
              |   cics_stmt_enable:s     {: RESULT = s; :}  
              |   cics_stmt_extract:s    {: RESULT = s; :} 
              |   cics_stmt_inquire:s    {: RESULT = s; :} 
              |   cics_stmt_perform:s    {: RESULT = s; :} 
              |   cics_stmt_resync:s     {: RESULT = s; :}  
              |   cics_stmt_set:s        {: RESULT = s; :}
              ;     
               
cics_stmt_common ::= cics_stmt_one              
                 |   cics_stmt_two              
                 |   cics_stmt_gds              
                 |   cics_stmt_bif              
                 |   cics_stmt_wait             
                 |   cics_stmt_issue            
                 |   cics_stmt_start              
                 |   cics_stmt_doc              
                 |   cics_stmt_address              
                 |   cics_stmt_counter          
                 |   cics_stmt_monitor          
                 |   cics_stmt_syncpoint        
                 |   cics_stmt_assign           
                 |   cics_stmt_send             
                 |   cics_stmt_connect          
                 |   cics_stmt_converse          
                 |   cics_stmt_get              
                 |   cics_stmt_delete           
                 |   cics_stmt_receive               
                 |   cics_stmt_suspend          
                 ;


/**********************************************************************/
/*                        GRUPOS                                     */
/**********************************************************************/

cics_group_abend ::= ABEND:w           {: stmt = new StmtCICS(w);         :}
                 |   HANDLE:w ABEND:w2 {: stmt = new StmtCICS(w.add(w2)); :}    
                 ;
         
cics_group_auth ::= CHANGE:w PASSWORD:w2  {: stmt = new StmtCICS(w.add(w2)); :}    
                |   SIGNOFF:w             {: stmt = new StmtCICS(w);         :}
                |   SIGNON:w              {: stmt = new StmtCICS(w);         :}
                |   VERIFY:w PASSWORD:w2  {: stmt = new StmtCICS(w.add(w2)); :}    
                ;
           
cics_group_bms ::= PURGE MESSAGE:w  {: stmt = new StmtCICS(w); :}
               |   ROUTE:w          {: stmt = new StmtCICS(w); :}
               ;
                                                         
cics_group_bts ::= ACQUIRE:w                         {: stmt = new StmtCICS(w);         :}
               |   ADD:w         SUBEVENT:w2         {: stmt = new StmtCICS(w.add(w2)); :}    
               |   CHECK:w       cics_check:w2       {: stmt = new StmtCICS(w.add(w2)); :}    
               |   DEFINE:w      cics_define:w2      {: stmt = new StmtCICS(w.add(w2)); :}    
               |   ENDBROWSE:w   cics_endbrowse:w2   {: stmt = new StmtCICS(w.add(w2)); :}    
               |   FORCE:w       TIMER:w2            {: stmt = new StmtCICS(w.add(w2)); :}    
               |   GETNEXT:w     cics_getnext:w2     {: stmt = new StmtCICS(w.add(w2)); :}       
               |   LINK:w        cics_link:w2        {: stmt = new StmtCICS(w.add(w2)); :}    
               |   REMOVE:w      SUBEVENT:w2         {: stmt = new StmtCICS(w.add(w2)); :}    
               |   RESET:w       cics_reset:w2       {: stmt = new StmtCICS(w.add(w2)); :}    
               |   RESUME:w                          {: stmt = new StmtCICS(w);         :}
               |   RETRIEVE:w    cics_retrieve:w2    {: stmt = new StmtCICS(w.add(w2)); :}    
               |   RUN:w                             {: stmt = new StmtCICS(w);         :}
               |   STARTBROWSE:w cics_startbrowse:w2 {: stmt = new StmtCICS(w.add(w2)); :}    
               |   TEST:w        EVENT:w2            {: stmt = new StmtCICS(w.add(w2)); :}    
               ;

cics_group_diag ::= DUMP:w  TRANSACTION:w2 {: stmt = new StmtCICS(w.add(w2)); :}    
                |   ENTER:w TRACENUM:w2    {: stmt = new StmtCICS(w.add(w2)); :}    
                ;

cics_group_excep ::= HANDLE:w CONDITION:w2 {: stmt = new StmtCICS(w.add(w2)); :}    
                 |   IGNORE:w CONDITION:w2 {: stmt = new StmtCICS(w.add(w2)); :}    
                 |   POP:w    HANDLE:w2    {: stmt = new StmtCICS(w.add(w2)); :}    
                 |   PUSH:w   HANDLE:w2    {: stmt = new StmtCICS(w.add(w2)); :}    
                 ;
                 
cics_group_file ::= ENDBR:w    {: stmt = new StmtCICS(w); :}
                |   READ:w     {: stmt = new StmtCICS(w); :}
                |   READNEXT:w {: stmt = new StmtCICS(w); :}
                |   READPREV:w {: stmt = new StmtCICS(w); :}
                |   RESETBR:w  {: stmt = new StmtCICS(w); :}
                |   REWRITE:w  {: stmt = new StmtCICS(w); :}
