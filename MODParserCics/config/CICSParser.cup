cics_options ::= cics_options_lst:c 
             |                      
             ;
             
cics_options_lst ::= cics_option:o                  {: RESULT = stmt.addOption(currOption); :}
                 |   cics_options_lst cics_option:o {: RESULT = stmt.addOption(currOption); :} 
                 ;
                                 
cics_option ::= cics_option_name:o cics_parm_opt  
            ;

cics_option_name ::= ID:w                  {: currOption = new Option(w); :}
                 |   cics_reserved_word:w  {: currOption = new Option(w); :}
                 ;

cics_reserved_word ::= LENGTH:w        {: RESULT = w; :}
                   |   PROGRAM:w       {: RESULT = w; :}
                   ;
         
                                                     
cics_parm_opt ::= LPAR cics_parms:w RPAR     {: currOption.setFunction(); 
                                                currOption.addParm(w);    
                                             :}
          |                              
          ;             

cics_parms ::= cics_parm:w               {: RESULT = w;         :} 
           |   cics_parms:w cics_parm:w2 {: RESULT = w.add(w2); :} 
           ;
            
cics_parm ::= ident_base:w                 {: RESULT = w; :}
          |   number:w                     {: RESULT = w; :}
          |   LITERAL:w                    {: RESULT = w; :}
          |   LENGTH:w1 OF:w2 ident_base:w {: RESULT = w.setPrefix(w1.add(w2)); :}
          |   LPAR cics_parms:w RPAR       {: RESULT = w.setFunction();         :}
          ;

ident_base ::= ID:w padres_lst:w2 {: RESULT = w.add(w2); :} 
           ;

number ::= ENTERO:w    {: RESULT = w; :}
       |   DECIMAL:w   {: RESULT = w; :} 
       ;                                                      
       
       

padres_lst ::= padres:p  {: RESULT = p;    :}
           |             {: RESULT = null; :}
           ;

padres ::= padre:w            {: RESULT = w;         :}
       |   padres:w padre:w2  {: RESULT = w.add(w2); :}
       ;
                    
padre ::= padre_indicator:p ID:w {: RESULT = w.setPrefix(p);    :}
      ;

padre_indicator ::= OF:w {: RESULT = w; :}
                |   IN:w {: RESULT = w; :}
                ;
       